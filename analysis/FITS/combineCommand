combineCards.py Zinvcat=datacard_STAT_Rho_Zinvcat_2018.txt Wcat=datacard_STAT_Rho_Wcat_2018.txt VBFcat=datacard_STAT_Rho_VBFcat_2018.txt VBFcatlow=datacard_STAT_Rho_VBFcatlow_2018.txt > datacard_STAT_Rho_comb.txt
combine -M AsymptoticLimits -t -1 datacard_STAT_Rho_comb.txt

combineCards.py Zinvcat=datacard_STAT_Phi_Zinvcat_2018.txt Wcat=datacard_STAT_Phi_Wcat_2018.txt VBFcat=datacard_STAT_Phi_VBFcat_2018.txt VBFcatlow=datacard_STAT_Phi_VBFcatlow_2018.txt > datacard_STAT_Phi_comb.txt
