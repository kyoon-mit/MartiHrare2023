combine -M AsymptoticLimits -t -1 DATACARDS/datacard_STAT_Phi_GFcat_2018.txt  > results.txt
combine -M AsymptoticLimits -t -1 DATACARDS/datacard_STAT_Rho_GFcat_2018.txt  >> results.txt

combine -M AsymptoticLimits -t -1 DATACARDS/datacard_STAT_Phi_VBFcat_2018.txt  >> results.txt
combine -M AsymptoticLimits -t -1 DATACARDS/datacard_STAT_Rho_VBFcat_2018.txt  >> results.txt

combine -M AsymptoticLimits -t -1 DATACARDS/datacard_STAT_Phi_VBFcatlow_2018.txt  >> results.txt
combine -M AsymptoticLimits -t -1 DATACARDS/datacard_STAT_Rho_VBFcatlow_2018.txt  >> results.txt

combine -M AsymptoticLimits -t -1 DATACARDS/datacard_STAT_Phi_Zinvcat_2018.txt >> results.txt
combine -M AsymptoticLimits -t -1 DATACARDS/datacard_STAT_Rho_Zinvcat_2018.txt >> results.txt

combine -M AsymptoticLimits -t -1 DATACARDS/datacard_STAT_Phi_Wcat_2018.txt >> results.txt
combine -M AsymptoticLimits -t -1 DATACARDS/datacard_STAT_Rho_Wcat_2018.txt >> results.txt

combine -M AsymptoticLimits -t -1 DATACARDS/datacard_STAT_Phi_Zcat_2018.txt >> results.txt
combine -M AsymptoticLimits -t -1 DATACARDS/datacard_STAT_Rho_Zcat_2018.txt >> results.txt


combineCards.py Zinvcat=DATACARDS/datacard_STAT_Rho_Zinvcat_2018.txt Wcat=DATACARDS/datacard_STAT_Rho_Wcat_2018.txt Zcat=DATACARDS/datacard_STAT_Rho_Zcat_2018.txt VBFcat=DATACARDS/datacard_STAT_Rho_VBFcat_2018.txt VBFcatlow=DATACARDS/datacard_STAT_Rho_VBFcatlow_2018.txt > DATACARDS/datacard_STAT_Rho_comb.txt
combine -M AsymptoticLimits -t -1 DATACARDS/datacard_STAT_Rho_comb.txt

combineCards.py Zinvcat=DATACARDS/datacard_STAT_Phi_Zinvcat_2018.txt Wcat=DATACARDS/datacard_STAT_Phi_Wcat_2018.txt Zcat=DATACARDS/datacard_STAT_Phi_Zcat_2018.txt VBFcat=DATACARDS/datacard_STAT_Phi_VBFcat_2018.txt VBFcatlow=DATACARDS/datacard_STAT_Phi_VBFcatlow_2018.txt > DATACARDS/datacard_STAT_Phi_comb.txt
combine -M AsymptoticLimits -t -1 DATACARDS/datacard_STAT_Phi_comb.txt
